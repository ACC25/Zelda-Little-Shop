<div class="admin-orders">
  <% @orders.each do |order| %>
  <%= link_to "Order: #{order.id}", admin_order_path(order.id) %>
  Status: <%= order.status %>
  <%= link_to "Complete", admin_order_path(order.id), method: :put, class:"btn btn-success" %>
  <%= link_to "Delete", admin_order_path(order.id), method: :delete, class:"btn btn-danger" %>
  <br>
  <% end %>
</div>
